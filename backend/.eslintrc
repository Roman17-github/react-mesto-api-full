{
    "env": {
        "browser": true,
        "es2021": true,
        "es6": true,
        "node": true,
        "amd": true
    },
    "extends": [
        "eslint:recommended",
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "off",
        "no-underscore-dangle": [1, { "allow": ["_id"] }]
    }
}